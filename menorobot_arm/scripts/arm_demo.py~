	rospy.loginfo('shoulder_'+str(sh))
	rospy.loginfo('elbow_'+str(e1))
	rospy.loginfo('wrist_'+str(e2))
	rospy.loginfo('left_finger_'+str(lf))
	rospy.loginfo('right_finger_'+str(rf))
